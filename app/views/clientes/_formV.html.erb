<%= form_with(model: veiculo, local: true) do |form| %>

  <div class="field">
    <%= form.label :placa %>
    <%= form.text_field :placa, id: :veiculo_placa %>
  </div>

  <div class="field">
    <%= form.label :marca %>
    <%= form.text_field :marca, id: :veiculo_marca %>
  </div>

  <div class="field">
    <%= form.label :modelo %>
    <%= form.text_field :modelo, id: :veiculo_modelo %>
  </div>

  <div class="field">
    <%= form.label :ano %>
    <%= form.text_field :ano, id: :veiculo_ano %>
  </div>

  <div class="field">
    <%= form.label :cor %>
    <%= form.text_field :cor, id: :veiculo_cor %>
  </div>

  <div class="field">
    <%= form.label :id_cliente_id %>
    <%= form.text_field :id_cliente_id, id: :veiculo_id_cliente_id %>
  </div>
  
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
